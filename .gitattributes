# Normalize EOL for all files that Git considers text files.
* text=auto eol=lf
*.ap filter=lfs diff=lfs merge=lfs -text
*.git filter=lfs diff=lfs merge=lfs -text
*.gitignore filter=lfs diff=lfs merge=lfs -text
*.import filter=lfs diff=lfs merge=lfs -text
*.godot filter=lfs diff=lfs merge=lfs -text
._Scenes filter=lfs diff=lfs merge=lfs -text
._Assets filter=lfs diff=lfs merge=lfs -text
._Scripts filter=lfs diff=lfs merge=lfs -text
*.png filter=lfs diff=lfs merge=lfs -text
